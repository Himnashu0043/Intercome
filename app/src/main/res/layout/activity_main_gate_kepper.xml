                app:layout_constraintEnd_toStartOf="@+id/iv_gateKeeper_gatePass"
                app:layout_constraintStart_toEndOf="@+id/iv_gateKeeper_home"
                app:layout_constraintTop_toTopOf="@+id/iv_gateKeeper_home"
                app:srcCompat="@drawable/visitor_unactive" />

            <ImageView
                android:id="@+id/iv_gateKeeper_gatePass"
                android:layout_width="0dp"
                android:layout_height="@dimen/dp_50"
                app:layout_constraintBottom_toBottomOf="@+id/iv_gateKeeper_visitor"
                app:layout_constraintEnd_toStartOf="@+id/iv_gateKeeper_profile"
                app:layout_constraintStart_toEndOf="@+id/iv_gateKeeper_visitor"
                app:layout_constraintTop_toTopOf="@+id/iv_gateKeeper_visitor"
                app:srcCompat="@drawable/gatepass_unactive" />

            <ImageView
                android:id="@+id/iv_gateKeeper_profile"
                android:layout_width="70dp"
                android:layout_height="0dp"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toBottomOf="@+id/iv_gateKeeper_gatePass"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/iv_gateKeeper_gatePass"
                app:layout_constraintTop_toTopOf="@+id/iv_gateKeeper_gatePass"
                app:srcCompat="@drawable/new_gatekeeper_un_profile" />-->
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigationView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        android:visibility="visible">

        <include
            android:id="@+id/nav"
            layout="@layout/user_nav_drw" />
    </com.google.android.material.navigation.NavigationView>
