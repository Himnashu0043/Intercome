<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".owner.activity.ownerPropertyToLetSale.OwnerPropertyToletSaleActivity">

    <include
        android:id="@+id/propertyToolbar"
        layout="@layout/common_toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/to_let_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_200sdp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/propertyToolbar">

        <ImageView
            android:id="@+id/imageView64"
            android:layout_width="0dp"
            android:layout_height="@dimen/_200sdp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/bg_img" />

        <TextView
            android:id="@+id/textView113"
            style="@style/normaltxt_24sp_extrabold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/to_let"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/textView114"
            style="@style/normaltxt_16sp_extrabold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toBottomOf="@+id/textView113" />

        <TextView
            android:id="@+id/tvflatno"
            style="@style/normaltxt_12sp_Semibold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:gravity="center_horizontal"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toBottomOf="@+id/textView114" />

        <TextView
            android:id="@+id/textView115"
            style="@style/normaltxt_14sp_Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginTop="4dp"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toBottomOf="@+id/tvflatno" />

        <androidx.cardview.widget.CardView
            android:layout_width="0dp"
            android:layout_height="@dimen/_46sdp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"

            android:layout_marginBottom="24dp"
            app:cardCornerRadius="@dimen/_12sdp"
            app:cardElevation="@dimen/_4sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <TextView
                style="@style/normaltxt_14sp_bold"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/list_your_property"
                android:textColor="@color/orange" />
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/sale_lay"
        android:layout_width="0dp"
        android:layout_height="@dimen/_200sdp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="@+id/to_let_lay"
        app:layout_constraintStart_toStartOf="@+id/to_let_lay"
        app:layout_constraintTop_toBottomOf="@+id/to_let_lay">

        <ImageView
            android:id="@+id/imageView641"
            android:layout_width="0dp"
            android:layout_height="@dimen/_200sdp"
            android:scaleType="fitXY"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/bg_img" />

        <TextView
            android:id="@+id/textView1131"
            style="@style/normaltxt_24sp_extrabold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/sale"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView641"
            app:layout_constraintStart_toStartOf="@+id/imageView641"
            app:layout_constraintTop_toTopOf="@+id/imageView641" />

        <TextView
            android:id="@+id/textView1141"
            style="@style/normaltxt_16sp_extrabold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="center_horizontal"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView641"
            app:layout_constraintStart_toStartOf="@+id/imageView641"
            app:layout_constraintTop_toBottomOf="@+id/textView1131" />

        <TextView
            android:id="@+id/tvFlatParkingNo"
            style="@style/normaltxt_12sp_Semibold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView641"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView1141" />

        <TextView
            android:id="@+id/textView1151"
            style="@style/normaltxt_14sp_Regular"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView641"
            app:layout_constraintStart_toStartOf="@+id/imageView641"
            app:layout_constraintTop_toBottomOf="@+id/tvFlatParkingNo" />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView18"
            android:layout_width="0dp"
            android:layout_height="@dimen/_46sdp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="24dp"
            app:cardCornerRadius="@dimen/_12sdp"
            app:cardElevation="@dimen/_4sdp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView641"
            app:layout_constraintEnd_toEndOf="@+id/imageView641"
            app:layout_constraintStart_toStartOf="@+id/imageView641">

            <TextView
                style="@style/normaltxt_14sp_bold"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/list_your_property"
                android:textColor="@color/orange" />
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>