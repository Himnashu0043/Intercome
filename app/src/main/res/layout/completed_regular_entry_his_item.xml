<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:cardCornerRadius="8dp"
        app:cardElevation="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/imageView93"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:scaleType="fitXY"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:shapeAppearanceOverlay="@style/service_10_imge_shapeable_overloay"
                app:srcCompat="@drawable/profile_img" />

            <TextView
                android:id="@+id/textView175"
                style="@style/normaltxt_14sp_Regular"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="16dp"
                android:text="Zeeshan Alam"
                app:layout_constraintEnd_toStartOf="@+id/textView178511"
                app:layout_constraintStart_toEndOf="@+id/imageView93"
                app:layout_constraintTop_toTopOf="@+id/imageView93" />

            <TextView
                android:id="@+id/textView176"
                style="@style/normaltxt_14sp_Regular"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="01765750808"
                app:layout_constraintEnd_toStartOf="@+id/tvComletedInTime"
                app:layout_constraintStart_toStartOf="@+id/textView175"
                app:layout_constraintTop_toBottomOf="@+id/textView175" />

            <TextView
                android:id="@+id/textView177"
                style="@style/normaltxt_12sp_Regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Delivery | Regular Entry"
                android:textColor="@color/new_dark_sileti"
                app:layout_constraintStart_toStartOf="@+id/imageView93"
                app:layout_constraintTop_toBottomOf="@+id/imageView93" />

            <TextView
                android:id="@+id/textView178"
                style="@style/normaltxt_12sp_Regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="16dp"
                android:text="TF-244, Yashoda ...."
                android:textColor="@color/new_dark_sileti"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/textView177"
                app:layout_constraintTop_toBottomOf="@+id/textView177" />

            <TextView
                android:id="@+id/textView178511"
                style="@style/normaltxt_12sp_bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/last_in"
                android:textColor="@color/new_dark_sileti"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView175" />

            <TextView
                android:id="@+id/tvComletedInTime"
                style="@style/normaltxt_12sp_Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right|center_vertical"
                android:text="05:00 PM, 03/06/2023"
                android:textColor="@color/new_dark_sileti"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/textView176"
                app:layout_constraintEnd_toEndOf="@+id/textView178511"
                app:layout_constraintTop_toBottomOf="@+id/textView178511" />


            <TextView
                android:id="@+id/textView17851"
                style="@style/normaltxt_12sp_bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:text="@string/last_out"
                android:textColor="@color/new_dark_sileti"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView177" />

            <TextView
                android:id="@+id/tvComletedOutTime"
                style="@style/normaltxt_12sp_Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right|center_vertical"
                android:text="05:00 PM, 03/06/2023"
                android:textColor="@color/new_dark_sileti"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/textView178"
                app:layout_constraintEnd_toEndOf="@+id/textView17851"
                app:layout_constraintTop_toBottomOf="@+id/textView17851" />



        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>