    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_distance"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        app:layout_constraintEnd_toStartOf="@+id/layout_bedroom"
        app:layout_constraintStart_toStartOf="@+id/iv_property"
        app:layout_constraintTop_toBottomOf="@+id/tv_distance">

        <ImageView
            android:id="@+id/iv_distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8sdp"
            android:background="@drawable/bg_login_type_btn"
            android:backgroundTint="#F0FAF2"
            android:minWidth="@dimen/_24sdp"
            android:minHeight="@dimen/_24sdp"
            android:padding="@dimen/_8sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_distance" />

        <TextView
            android:id="@+id/tv_fit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_4sdp"
            android:fontFamily="@font/medium"
            android:text="1,107 ft"
            android:textColor="#20222C"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_distance"
            app:layout_constraintStart_toEndOf="@+id/iv_distance"
            app:layout_constraintTop_toTopOf="@+id/iv_distance" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_bedroom"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        app:layout_constraintEnd_toStartOf="@+id/layout_bathroom"
        app:layout_constraintStart_toEndOf="@+id/layout_distance"
        app:layout_constraintTop_toBottomOf="@+id/tv_distance">

        <ImageView
            android:id="@+id/iv_bedroom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8sdp"
            android:background="@drawable/bg_login_type_btn"
            android:backgroundTint="#F0FAF2"
            android:minWidth="@dimen/_24sdp"
            android:minHeight="@dimen/_24sdp"
            android:padding="@dimen/_8sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_bed" />

        <TextView
            android:id="@+id/tv_bedroom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_4sdp"
            android:fontFamily="@font/medium"
            android:text="2 bedroom"
            android:textColor="#20222C"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_bedroom"
            app:layout_constraintStart_toEndOf="@+id/iv_bedroom"
            app:layout_constraintTop_toTopOf="@+id/iv_bedroom" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_bathroom"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/layout_bedroom"
        app:layout_constraintTop_toBottomOf="@+id/tv_distance">

        <ImageView
            android:id="@+id/iv_bathroom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8sdp"
            android:background="@drawable/bg_login_type_btn"
            android:backgroundTint="#F4EAFB"
            android:minWidth="@dimen/_24sdp"
            android:minHeight="@dimen/_24sdp"
            android:padding="@dimen/_4sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_bathroom" />

        <TextView
            android:id="@+id/tv_bathroom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_4sdp"
            android:fontFamily="@font/medium"
            android:text="2 Bathrrom"
            android:textColor="#20222C"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_bathroom"
            app:layout_constraintStart_toEndOf="@+id/iv_bathroom"
            app:layout_constraintTop_toTopOf="@+id/iv_bathroom" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_16sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:background="@drawable/bg_edit_text"
        android:paddingBottom="@dimen/_8sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_bathroom">

        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/iv_profile"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_30sdp"
            android:layout_marginStart="@dimen/_8sdp"
            android:layout_marginTop="@dimen/_8sdp"
            android:scaleType="centerCrop"
            app:shapeAppearance="@style/ShapeAppearanceOverlay.App.CornerSize50Percent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:shapeAppearanceOverlay="@style/Circular"
            app:srcCompat="@drawable/properties_parking" />

        <TextView
            android:id="@+id/tv_profile_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_8sdp"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginEnd="16dp"
            android:fontFamily="@font/semi_bold"
            android:textColor="#20222C"
            android:textSize="@dimen/_12ssp"
            app:layout_constraintEnd_toStartOf="@+id/tv_owner"
            app:layout_constraintStart_toEndOf="@+id/iv_profile"
            app:layout_constraintTop_toTopOf="@+id/iv_profile" />

        <TextView
            android:id="@+id/tv_owner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_8sdp"
            android:fontFamily="@font/semi_bold"
            android:text="Owner"
            android:textColor="#858585"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_profile_name"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/iv_profile" />

        <TextView
            android:id="@+id/tv_mobile_no"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_8sdp"
            android:layout_marginEnd="8dp"
            android:drawableStart="@drawable/ic_receiver"
            android:drawablePadding="@dimen/_4sdp"
            android:fontFamily="@font/semi_bold"
            android:gravity="left"
            android:textColor="#858585"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintEnd_toStartOf="@+id/tv_email"
            app:layout_constraintStart_toStartOf="@+id/iv_profile"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile" />


        <TextView
            android:id="@+id/tv_email"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="@dimen/_8sdp"
            android:drawableStart="@drawable/ic_email"
            android:drawablePadding="@dimen/_4sdp"
            android:fontFamily="@font/semi_bold"
            android:gravity="left"

            android:textColor="#858585"
            android:textSize="@dimen/_10ssp"
            app:layout_constraintBottom_toBottomOf="@+id/tv_mobile_no"
            app:layout_constraintEnd_toEndOf="@+id/tv_owner"
            app:layout_constraintStart_toEndOf="@+id/tv_mobile_no"
            app:layout_constraintTop_toBottomOf="@+id/iv_profile" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/tv_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/bold"
        android:text="Details"
        android:textColor="#20222C"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintStart_toStartOf="@+id/layout_profile"
        app:layout_constraintTop_toBottomOf="@+id/layout_profile" />

    <TextView
        android:id="@+id/tv_details_desc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/regular"
        android:textColor="#858585"
        android:textSize="@dimen/_11ssp"
        app:layout_constraintEnd_toEndOf="@+id/layout_bathroom"
        app:layout_constraintStart_toStartOf="@+id/tv_details"
        app:layout_constraintTop_toBottomOf="@+id/tv_details" />

    <TextView
        android:id="@+id/tv_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/bold"
        android:text="Price"
        android:textColor="#20222C"
        android:textSize="@dimen/_12ssp"
        app:layout_constraintStart_toStartOf="@+id/layout_profile"
        app:layout_constraintTop_toBottomOf="@+id/tv_details_desc" />

    <TextView
        android:id="@+id/tv_duration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/medium"
        android:text="Monthly"
        android:textColor="#858585"
        android:textSize="@dimen/_10ssp"
        app:layout_constraintStart_toStartOf="@+id/tv_details"
        app:layout_constraintTop_toBottomOf="@+id/tv_price" />

    <TextView
        android:id="@+id/tv_price_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/extra_bold"
        android:text="৳640"
        android:textColor="#20222C"
        android:textSize="@dimen/_14ssp"
        app:layout_constraintBottom_toBottomOf="@+id/tv_duration"
        app:layout_constraintEnd_toEndOf="@+id/tv_details_desc"
        app:layout_constraintTop_toTopOf="@+id/tv_duration" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_communication"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_24sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_price_value">

        <ImageView
            android:id="@+id/iv_calling"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_calling_and_message"
            android:minWidth="@dimen/_40sdp"
            android:minHeight="@dimen/_40sdp"
            android:scaleType="center"
            app:layout_constraintEnd_toStartOf="@id/iv_message"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_calling" />

        <ImageView
            android:id="@+id/iv_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_8sdp"
            android:background="@drawable/bg_calling_and_message"
            android:minWidth="@dimen/_40sdp"
            android:minHeight="@dimen/_40sdp"
            android:scaleType="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_calling"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_chatting" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>